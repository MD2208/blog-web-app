<%-include("partials/header");-%>
<h1>Compose</h1>

<form  action="#" method="post">
  <div class="form-group">
    <label for="title">Title</label><br>
    <input type="text" class="form-control" id="title" name="composeTitle" autocomplete="off">
  </div>

  <div class="form-group">
    <label for="post">Post</label><br>
    <textarea id="post" class="form-control" name="composePost" rows="8" ></textarea>
  </div>
    <button type="submit" class="btn btn-primary" name="button">Publish!</button>


</form>

<%- include("partials/footer");-%>
